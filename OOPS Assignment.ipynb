    "class pow:\n",
    "    def __init__(self):\n",
    "        self.x = int(input(\"Enter the value:\"))\n",
    "        self.n = int(input(\"Enter the power value:\"))\n",
    "       \n",
    "    \n",
    "    def power(self):\n",
    "         return self.x**self.n\n",
    "result = pow()\n",
    "\n",
    "print(\"The output is:\", result.power())"
   ]
